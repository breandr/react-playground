extends: eslint-config-airbnb
rules:
    new-cap: 0
    no-use-before-define: 0
    indent: [2, 2]
    comma-dangle: [2, never]
    lines-around-comment: 0
    valid-jsdoc:
        - 2
        -
            requireReturn: false
            requireParamDescription: true
            requireReturnDescription: true
            prefer:
                return: returns
    # 'react/prop-types': 0
    'react/sort-comp': 0
